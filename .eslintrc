{
  "rules": {
    "camelcase": 1,
    "new-cap": 1,
    "no-console": 1,
    "no-delete-var": 0,
    "no-use-before-define": 0,
    "no-mixed-spaces-and-tabs": [2, true],
    "no-process-exit": 0,
    "no-space-before-semi": 1,
    "no-underscore-dangle": 1,
    "quotes": [2, "double"],
    "space-infix-ops": 1,
    "key-spacing": 0,
    "strict": 1,
    "no-comma-dangle": 1,
    "no-cond-assign": 1,
    "no-dupe-keys": 1,
    "no-empty": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-irregular-whitespace": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "curly": 1,
    "default-case": 1,
    "no-caller": 1,
    "eqeqeq": 1,
    "no-empty-label": 1,
    "no-extend-native": 1,
    "no-fallthrough": 1,
    "no-iterator": 1,
    "no-native-reassign": 1, 
    "no-new": 1, 
    "handle-callback-err": 1,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-self-compare": 1,
    "radix": 1,
    "vars-on-top": 1,
    "yoda": 1,
    "global-strict": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1
  },
  "env": {
    "node": true
  }
}
